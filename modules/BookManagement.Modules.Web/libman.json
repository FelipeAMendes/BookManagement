{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.2.2",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/bootstrap-grid.css",
        "css/bootstrap-reboot.css",
        "css/bootstrap-utilities.css",
        "css/bootstrap.css",
        "js/bootstrap.js"
      ]
    },
    {
      "library": "jquery@3.6.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "additional-methods.js",
        "localization/messages_uk.js",
        "localization/messages_pt_BR.js",
        "jquery.validate.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.js"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr/",
      "files": [
        "toastr.min.js",
        "toastr.css"
      ]
    },
    {
      "library": "font-awesome@6.2.1",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "js/all.js",
        "css/all.css",
        "sprites/brands.svg",
        "sprites/regular.svg",
        "sprites/solid.svg",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff2",
        "webfonts/fa-v4compatibility.ttf",
        "webfonts/fa-v4compatibility.woff2"
      ]
    }
  ]
}